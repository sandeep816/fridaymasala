{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/404/?$",
			"dest": "_render"
		},
		{
			"src": "^/actor/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/celebrities/?$",
			"dest": "_render"
		},
		{
			"src": "^/movie/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/movies/now-playing/?$",
			"dest": "_render"
		},
		{
			"src": "^/movies/popular/?$",
			"dest": "_render"
		},
		{
			"src": "^/movies/top-rated/?$",
			"dest": "_render"
		},
		{
			"src": "^/movies/trending/?$",
			"dest": "_render"
		},
		{
			"src": "^/search/?$",
			"dest": "_render"
		},
		{
			"src": "^/sitemap\\.xml/?$",
			"dest": "_render"
		},
		{
			"src": "^/$",
			"dest": "_render"
		},
		{
			"src": "^/.*$",
			"dest": "_render",
			"status": 404
		}
	]
}